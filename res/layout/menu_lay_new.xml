<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:background="@android:color/transparent">
            
    <LinearLayout 
			android:id="@+id/ll_content_close"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:background="#90000000"
            android:orientation="vertical" >

    
<LinearLayout 
			android:id="@+id/content"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:background="#2C3948"
            android:layout_marginRight="50dp"
            android:orientation="vertical" >

    
    <RelativeLayout android:id="@+id/close_lay"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#2C3948">
    <Button
        android:id="@+id/closemenu"
        android:layout_width="20dp"
        android:layout_height="15dp"
        android:background="@drawable/menu_icon"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:textColor="#FFF"
        android:textSize="25sp" />
    
    </RelativeLayout>
            
<ScrollView
    android:id="@+id/sv_items"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="#2C3948"
        android:scrollbars="none"
        >
    
        <LinearLayout
        android:id="@+id/list_lay1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="#2C3948"
        
        android:orientation="vertical" >
        
             <RelativeLayout
                android:id="@+id/option_logo"
        android:layout_height="100dp"
        android:layout_width="fill_parent"
        
        android:background="#2C3948" >
             <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="100dp"
                    android:layout_height="96dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/seva60_menu_logo" />
            
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_hum_training"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:layout_marginTop="30dp"
        android:background="#283341" >

                <ImageView
                    android:id="@+id/menu_icon"
                    android:layout_width="25dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/pill_logo" />

                <TextView
                    android:id="@+id/tv_fin_advisory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/menu_icon"
                    android:text="PILL REMINDER"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />


            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/option9"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#293644" >

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="25dp"
                    android:layout_height="28dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/offers_5" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView9"
                    android:text="SPECIAL OFFERS"
                    android:textSize="18sp"
                    android:textColor="#FFF"
                    android:layout_marginLeft="15dp" />
           
            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_doctor_speaks"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#283341" >

                <ImageView
                    android:id="@+id/riv_saathi2_picture"
                    android:layout_width="25dp"
                    android:layout_height="17dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/sathi_logo" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/riv_saathi2_picture"
                    android:text="SAATHI"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />
                
               

            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_financial_advisory"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#293644" >

                 <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="25dp"
                    android:layout_height="23dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/well_bing_logo" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView3"
                    android:text="WELLBEING"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />
             

            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_fitness"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#283341" >

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="25dp"
                    android:layout_height="31dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/knownledge_base_logo" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView4"
                    android:text="MEDIA CENTRE"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />
               

            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_technology"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#293644" >
                
                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/nearby_logo" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView5"
                    android:text="NEARBY"
                    android:textSize="18sp"
                    android:textColor="#FFF"
                    android:layout_marginLeft="15dp" />

               

            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_art_and_culture"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#283341" >

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="25dp"
                    android:layout_height="26dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/share_location_logo" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView6"
                    android:text="SHARE LOCATION"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />
            
            </RelativeLayout>
            
            <RelativeLayout
                android:id="@+id/rl_food"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:background="#293644" >

                 <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/utilities_logo" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView7"
                    android:text="UTILITIES"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />

            </RelativeLayout>
            
            <RelativeLayout
         android:id="@+id/rl_travel"
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:background="#283341"
        android:visibility="visible" >

                   <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="25dp"
                    android:layout_height="24dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/money_reminder_logo" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/imageView8"
                    android:text="REMINDER"
                    android:textColor="#FFF"
                    android:textSize="18sp"
                    android:layout_marginLeft="15dp" />
                   
              

            </RelativeLayout>
            
        </LinearLayout>

            </ScrollView>
            
        </LinearLayout>
   </LinearLayout>     

        
</RelativeLayout>